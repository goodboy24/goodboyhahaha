//模拟实现strcpy 
#include<stdio.h>
#include<stdlib.h>
#include<assert.h>
char* my_strcpy(char* dest, const char* scr)
{
	char* ret = dest;
	assert(dest);
	assert(scr);
	while (*dest++ = *scr++)
	{

	}
	return ret;
}
int main()
{
	char arr2[] = "abcdef";
	char arr1[] = "hihiboy";
	my_strcpy(arr1,arr2);
	printf("%s", arr1);
	system("pause");
	return 0;
}
